<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <style name="TextAppearance.App" parent="TextAppearance.Material3.BodyLarge" />

    <style name="TextAppearance.App.DisplayLarge" parent="TextAppearance.Material3.DisplayLarge">
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textSize">@dimen/font_size_hero_title</item>
        <item name="lineHeight">44sp</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_heading</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="TextAppearance.App.DisplayMedium" parent="TextAppearance.Material3.DisplayMedium">
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textSize">@dimen/font_size_headline_medium</item>
        <item name="lineHeight">36sp</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_heading</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="TextAppearance.App.DisplaySmall" parent="TextAppearance.Material3.DisplaySmall">
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textSize">@dimen/font_size_headline_small</item>
        <item name="lineHeight">32sp</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_heading</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="TextAppearance.App.HeadlineLarge" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">@dimen/font_size_headline_small</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_heading</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="TextAppearance.App.HeadlineMedium" parent="TextAppearance.Material3.HeadlineMedium">
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">@dimen/font_size_title_large</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_heading</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="TextAppearance.App.HeadlineSmall" parent="TextAppearance.Material3.HeadlineSmall">
        <item name="fontFamily">@font/poppins_medium</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">@dimen/font_size_title_medium</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_heading</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="TextAppearance.App.TitleLarge" parent="TextAppearance.Material3.TitleLarge">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">@dimen/font_size_body_large</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_body</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="TextAppearance.App.TitleMedium" parent="TextAppearance.Material3.TitleMedium">
        <item name="fontFamily">@font/poppins_medium</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">@dimen/font_size_body_medium</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_body</item>
        <item name="android:textColor">?attr/colorOnSurfaceVariant</item>
    </style>

    <style name="TextAppearance.App.BodyLarge" parent="TextAppearance.Material3.BodyLarge">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">@dimen/font_size_body_large</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_body</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="TextAppearance.App.BodyMedium" parent="TextAppearance.Material3.BodyMedium">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">@dimen/font_size_body_medium</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_body</item>
        <item name="android:textColor">?attr/colorOnSurfaceVariant</item>
    </style>

    <style name="TextAppearance.App.BodySmall" parent="TextAppearance.Material3.BodySmall">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">@dimen/font_size_caption</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_body</item>
        <item name="android:textColor">?attr/colorOnSurfaceVariant</item>
    </style>

    <style name="TextAppearance.App.LabelLarge" parent="TextAppearance.Material3.LabelLarge">
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">@dimen/font_size_button</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="Widget.App.Button" parent="Widget.Material3.Button">
        <item name="android:textAppearance">@style/TextAppearance.App.LabelLarge</item>
        <item name="android:paddingLeft">@dimen/button_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/button_padding_horizontal</item>
        <item name="android:paddingTop">@dimen/button_padding_vertical</item>
        <item name="android:paddingBottom">@dimen/button_padding_vertical</item>
        <item name="cornerRadius">@dimen/button_corner_radius</item>
        <item name="android:minHeight">@dimen/button_height_medium</item>
    </style>

    <style name="Widget.App.Button.Outlined" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:textAppearance">@style/TextAppearance.App.LabelLarge</item>
        <item name="android:paddingLeft">@dimen/button_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/button_padding_horizontal</item>
        <item name="android:paddingTop">@dimen/button_padding_vertical</item>
        <item name="android:paddingBottom">@dimen/button_padding_vertical</item>
        <item name="cornerRadius">@dimen/button_corner_radius</item>
        <item name="android:minHeight">@dimen/button_height_medium</item>
        <item name="strokeWidth">1dp</item>
        <item name="strokeColor">?attr/colorPrimary</item> </style>

    <style name="Widget.App.CardView.Elevated" parent="Widget.Material3.CardView.Elevated">
        <item name="cardElevation">@dimen/elevation_low</item>
        <item name="cardCornerRadius">@dimen/card_corner_radius_medium</item>
        <item name="android:layout_margin">@dimen/spacing_small</item>
    </style>
    <style name="Widget.App.CardView.Filled" parent="Widget.Material3.CardView.Filled">
        <item name="cardCornerRadius">@dimen/card_corner_radius_medium</item>
        <item name="android:layout_margin">@dimen/spacing_small</item>
        <item name="cardBackgroundColor">?attr/colorSurfaceContainerHighest</item> </style>
    <style name="Widget.App.CardView.Outlined" parent="Widget.Material3.CardView.Outlined">
        <item name="cardCornerRadius">@dimen/card_corner_radius_medium</item>
        <item name="android:layout_margin">@dimen/spacing_small</item>
        <item name="strokeColor">?attr/colorOutlineVariant</item>
    </style>

    <style name="Widget.App.Toolbar" parent="Widget.Material3.Toolbar.Surface">
        <item name="android:background">?attr/colorSurface</item> <item name="titleTextAppearance">@style/TextAppearance.App.TitleLarge</item> <item name="subtitleTextAppearance">@style/TextAppearance.App.BodyMedium</item>
        <item name="navigationIconTint">?attr/colorOnSurface</item> <item name="titleTextColor">?attr/colorOnSurface</item>
        <item name="android:elevation">@dimen/elevation_medium</item> </style>

    <style name="Widget.App.TabLayout" parent="Widget.Material3.TabLayout">
        <item name="android:background">?attr/colorSurface</item>
        <item name="tabIndicatorColor">?attr/colorPrimary</item>
        <item name="tabIndicatorHeight">2dp</item>
        <item name="tabTextAppearance">@style/TextAppearance.App.LabelLarge</item>
        <item name="tabSelectedTextAppearance">@style/TextAppearance.App.LabelLarge</item>
        <item name="tabSelectedTextColor">?attr/colorPrimary</item>
        <item name="tabTextColor">?attr/colorOnSurfaceVariant</item>
        <item name="tabIconTint">?attr/colorOnSurfaceVariant</item>
        <item name="tabIconTintMode">src_in</item>
        <item name="tabRippleColor">@color/tab_primary_ripple_color</item>
    </style>

    <style name="Widget.App.Switch" parent="Widget.Material3.CompoundButton.Switch">
        <item name="thumbTint">?attr/colorPrimary</item>
        <item name="trackTint">?attr/colorSurfaceVariant</item> </style>

    <style name="Widget.App.ImageView.Rounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/card_corner_radius_medium</item>
    </style>
    <style name="Widget.App.ImageViewContainer.Rounded" parent="Widget.App.CardView.Elevated">
        <item name="cardPreventCornerOverlap">false</item>
        <item name="cardUseCompatPadding">false</item>
        <item name="cardElevation">0dp</item> <item name="cardBackgroundColor">@android:color/transparent</item> </style>


    <style name="NavigationMenuText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingStart">@dimen/spacing_medium</item>
        <item name="android:paddingEnd">@dimen/spacing_medium</item>
        <item name="android:gravity">center_vertical</item>
        <item name="fontFamily">@font/poppins_medium</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">@dimen/font_size_body_medium</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="FooterTitleText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textSize">@dimen/font_size_body_large</item>
        <item name="android:layout_marginBottom">@dimen/spacing_medium</item>
    </style>

    <style name="FooterBodyText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textColor">?attr/colorOnSurfaceVariant</item>
        <item name="android:textSize">@dimen/font_size_body_medium</item>
        <item name="android:lineSpacingMultiplier">@dimen/line_spacing_multiplier_body</item>
        <item name="android:layout_marginBottom">@dimen/spacing_small</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="FooterLinkText" parent="FooterBodyText">
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="FooterSocialIcon">
        <item name="android:layout_width">@dimen/icon_size_large</item>
        <item name="android:layout_height">@dimen/icon_size_large</item>
        <item name="android:layout_marginEnd">@dimen/spacing_medium</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
        <item name="tint">?attr/colorPrimary</item>
    </style>

    <style name="PartnerLogoStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/icon_size_xlarge</item>
        <item name="android:layout_columnWeight">1</item>
        <item name="android:layout_gravity">center_horizontal|center_vertical</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="tint">?attr/colorOnSurfaceVariant</item> </style>

    <style name="Widget.App.BottomNavigationView" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="itemIconTint">@color/bottom_nav_item_tint_selector</item>
        <item name="itemTextColor">@color/bottom_nav_item_tint_selector</item>
        <item name="itemActiveIndicatorStyle">@style/Widget.App.BottomNavigationView.ActiveIndicator</item>
        <item name="labelVisibilityMode">labeled</item> </style>

    <style name="Widget.App.BottomNavigationView.ActiveIndicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:width">@dimen/m3_bottom_nav_item_active_indicator_width</item>
        <item name="android:height">@dimen/m3_bottom_nav_item_active_indicator_height</item>
        <item name="marginHorizontal">@dimen/m3_bottom_nav_item_active_indicator_margin_horizontal</item>
        <item name="shapeAppearance">@style/ShapeAppearance.Material3.Corner.Full</item> <item name="android:color">?attr/colorSecondaryContainer</item> </style>
</resources>